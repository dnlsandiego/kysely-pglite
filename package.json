{
  "name": "kysely-pglite",
  "version": "0.5.0",
  "description": "Kysely dialect for @electric-sql/pglite",
  "repository": "https://github.com/dnlsandiego/kysely-pglite",
  "keywords": [
    "@electric-sql/pglite",
    "kysely",
    "postgresql",
    "wasm",
    "database",
    "sql"
  ],
  "license": "MIT",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@8.14.2",
  "scripts": {
    "build": "tsup",
    "build:types": "tsc -d --declarationDir dist --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build && pnpm run build:types",
    "format": "prettier ./src --write",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/cli.js",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@electric-sql/pglite": "*",
    "kysely": "*",
    "kysely-codegen": "*"
  },
  "devDependencies": {
    "@repeaterjs/repeater": "^3.0.6",
    "@types/node": "^22.5.0",
    "cleye": "^1.3.2",
    "jiti": "2.0.0-beta.3",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "tsx": "^4.18.0",
    "type-fest": "^4.25.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.3"
  },
  "author": "Daniel Sandiego"
}
